#include <stdio.h>

int main()
{
    char op;
    double a,b;
    if(scanf(" %c",&op)!=1) return 0;
    if(scanf("%lf %lf",&a,&b)!=2)
return 0;
    switch(op)
{
        case '+': printf("%g\n", a+b);
break;
        case '-': printf("%g\n", a-b);
break;
        case '*': printf("%g\n", a*b);
break;
        case '/': if(b==0)printf("Division by zero\n"); 
else printf("%g\n", a/b); 
break;
        default: printf("Invalid operator\n");
    }
    return 0;
}
